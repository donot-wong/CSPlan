#require 7-Stored_File_Tampering.script;
#include constants.inc;
#include classPHPCodeInjection.inc;
var targetUrl = new TURL(scanURL.url);
targetUrl.getVar = "";
var injectionPatterns = new classInjectionPatterns();
var li = getStoredInjectionList(REFL_TEST_PHPCODEEV);
for (var i=0; i<li.count; i++) {	
	var storedInj = li.item(i);
	
	var sf = getSiteFileWithId(storedInj.reflectionId, loadHttp);
	var job = new THTTPJob();
	job.request.copyFrom(sf.request);
	
	var scheme = getSchemeWithId(storedInj.schemeId);
	targetUrl.path = scheme.path;
	
	var tester = new classPHPCodeInjection(targetUrl, injectionPatterns, scheme, storedInj.inputIndex, storedInj.variationIndex, job);
	tester.startTesting();	
	
	ScriptProgress(ComputeProgress(i, li.count));
}
