function alert(fname, version)
{	
	var ri = new TReportItem();
	ri.LoadFromFile(fname);
	ri.affects = "Web Server";
	ri.Details =  "Current version is : [dark]" + version + "[/dark]";
	AddReportItem(ri);	
}							 
